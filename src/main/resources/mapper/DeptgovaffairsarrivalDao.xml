<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.miniprogram.zhihuicunwu.dao.DeptgovaffairsarrivalDao">

    <resultMap type="com.miniprogram.zhihuicunwu.entity.Deptgovaffairsarrival" id="DeptgovaffairsarrivalMap">
        <result property="gaaid" column="GAAid" jdbcType="INTEGER"/>
        <result property="did" column="did" jdbcType="INTEGER"/>
    </resultMap>

    <!--查询单个-->
    <select id="queryById" resultMap="DeptgovaffairsarrivalMap">
        select
          GAAid, did
        from deptgovaffairsarrival
        where GAAid = #{gaaid}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="DeptgovaffairsarrivalMap">
        select
          GAAid, did
        from deptgovaffairsarrival
        <where>
            <if test="gaaid != null">
                and GAAid = #{gaaid}
            </if>
            <if test="did != null">
                and did = #{did}
            </if>
        </where>
        limit #{pageable.offset}, #{pageable.pageSize}
    </select>

    <!--统计总行数-->
    <select id="count" resultType="java.lang.Long">
        select count(1)
        from deptgovaffairsarrival
        <where>
            <if test="gaaid != null">
                and GAAid = #{gaaid}
            </if>
            <if test="did != null">
                and did = #{did}
            </if>
        </where>
    </select>

