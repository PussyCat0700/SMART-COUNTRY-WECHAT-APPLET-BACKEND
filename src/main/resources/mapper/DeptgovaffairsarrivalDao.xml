    <insert id="insert" keyProperty="gaaid" useGeneratedKeys="true">
        insert into deptgovaffairsarrival()
        values ()
    </insert>

    <insert id="insertBatch" keyProperty="gaaid" useGeneratedKeys="true">
        insert into deptgovaffairsarrival()
        values
        <foreach collection="entities" item="entity" separator=",">
        ()
        </foreach>
    </insert>

    <insert id="insertOrUpdateBatch" keyProperty="gaaid" useGeneratedKeys="true">
        insert into deptgovaffairsarrival()
        values
        <foreach collection="entities" item="entity" separator=",">
            ()
        </foreach>
        on duplicate key update
        
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update deptgovaffairsarrival
        <set>
        </set>
        where GAAid = #{gaaid}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from deptgovaffairsarrival where GAAid = #{gaaid}
    </delete>

</mapper>

